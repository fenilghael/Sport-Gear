@model SportsGearCMS.Models.Manufacturer
<h1>Manufacturer Details</h1>
<dl class="row">
    <dt class="col-sm-2">Name</dt>
    <dd class="col-sm-10">@Model.Name</dd>
    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.ContactEmail</dd>
</dl>
<a asp-action="Edit" asp-route-id="@Model.ManufacturerId" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back</a>
